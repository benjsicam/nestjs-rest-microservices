{
  "name": "api-gateway",
  "version": "1.0.0",
  "description": "REST API Gateway",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.5.3",
    "@nestjs/common": "6.11.6",
    "@nestjs/config": "0.2.2",
    "@nestjs/core": "6.11.6",
    "@nestjs/microservices": "6.11.6",
    "@nestjs/platform-express": "6.11.6",
    "express": "4.17.1",
    "grpc": "^1.24.2",
    "lodash": "4.17.15",
    "nestjs-pino": "1.1.3",
    "pino": "5.16.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.5.4"
  },
  "devDependencies": {
    "@nestjs/cli": "6.14.2",
    "@nestjs/schematics": "6.9.3",
    "@nestjs/testing": "6.11.6",
    "@types/express": "4.17.2",
    "@types/faker": "4.1.9",
    "@types/jest": "25.1.2",
    "@types/lodash": "4.14.149",
    "@types/node": "13.7.0",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.7.0",
    "eslint-plugin-prettier": "3.1.2",
    "faker": "4.1.0",
    "jest": "25.1.0",
    "jest-extended": "0.11.5",
    "pino-pretty": "3.5.0",
    "prettier": "1.19.1",
    "supertest": "4.0.2",
    "ts-jest": "25.2.0",
    "ts-loader": "6.2.1",
    "ts-node": "8.6.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.7.5"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:benjsicam/nestjs-rest-microservices.git"
  },
  "author": "Benj Sicam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benjsicam/nestjs-rest-microservices/issues"
  },
  "homepage": "https://github.com/benjsicam/nestjs-rest-microservices#readme",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": "test/*.+(test.ts)",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
